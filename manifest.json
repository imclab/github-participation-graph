{
  "name": "GitHub Participation Graph",
  "version": "0.1",
  "description": "Github participation graph improvement",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.github.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.github.com/*"],
      "js": ["jquery-1.8.1.min.js", "content.js"]
    }
  ],
  "manifest_version": 2
}
